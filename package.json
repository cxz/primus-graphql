{
  "name": "primus-graphql",
  "version": "1.2.0",
  "description": "Primus server and client plugins for GraphQL",
  "main": "index.js",
  "scripts": {
    "build": "make build",
    "test": "npm run test-unit && npm run test-browser",
    "test-unit": "npm run build && istanbul cover _mocha ./test && istanbul --text check-coverage --statements 100 --functions 100 --branches 100 --lines 100",
    "test-browser": "make build-browser && karma start test-browser/karmaconf.js",
    "test-watch": "nodemon -x mocha ./test",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tjmehta/primus-graphql.git"
  },
  "keywords": [
    "primus",
    "graphql",
    "server",
    "client",
    "query",
    "relay",
    "socket",
    "websocket"
  ],
  "author": "Tejesh Mehta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tjmehta/primus-graphql/issues"
  },
  "homepage": "https://github.com/tjmehta/primus-graphql",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-relay-plugin": "^0.9.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "co": "^4.6.0",
    "code": "^1.5.0",
    "engine.io": "^1.6.11",
    "engine.io-client": "^1.6.11",
    "graphql": "^0.6.0",
    "istanbul": "^0.4.4",
    "karma": "^1.0.0",
    "karma-browserify": "^5.0.5",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.0.1",
    "mocha": "^2.5.3",
    "primus": "^5.2.2",
    "proxyquire": "^1.7.9",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-relay": "^0.9.1",
    "shimmer": "^1.1.0",
    "sinon": "^1.17.4",
    "sinon-as-promised": "^4.0.0",
    "throw-next-tick": "^0.1.0"
  },
  "dependencies": {
    "101": "^1.6.0",
    "call-me-maybe": "^1.0.1",
    "debug": "^2.2.0",
    "error-to-json": "^1.0.1",
    "promise-retry": "^1.1.0",
    "uuid": "^2.0.2",
    "warning": "^3.0.0"
  },
  "peerDependencies": {
    "graphql": "^0.6.0",
    "react-relay": "^0.9.1"
  }
}
